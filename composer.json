{
  "name": "its/solidarity",
  "description": "solidarity",
  "license": "",
  "type": "project",
  "repositories": [
    {
      "type": "git",
      "url" : "https://bitbucket.org/dj_avolak/skeletor"
    }
  ],
  "require": {
    "dj_avolak/skeletor": "5.x-dev",
    "symfony/string": "^7",
    "laminas/laminas-session": "^2.21.0",
    "nesbot/carbon": "*",
    "phpoffice/phpspreadsheet": "*",
    "mailersend/mailersend": "^0.30.0",
    "turanjanin/serbian-transliterator": "*",
    "beberlei/doctrineextensions": "^1.5",
    "doctrine/orm": "^3.3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpmd/phpmd": "*",
    "roave/security-advisories": "dev-master",
    "rregeer/phpunit-coverage-check": "*"
  },
  "autoload": {
    "psr-4": {
      "Solidarity\\Backend\\": "packages/Backend/src",
      "Solidarity\\Frontend\\": "packages/Frontend/src",
      "Solidarity\\Mailer\\": "packages/Mailer/src",
      "Solidarity\\Transliterator\\": "packages/Transliterator/src",
      "Solidarity\\Donor\\": "packages/Donor/src",
      "Solidarity\\Transaction\\": "packages/Transaction/src",
      "Solidarity\\Educator\\": "packages/Educator/src",
      "Solidarity\\School\\": "packages/School/src",
      "Solidarity\\SchoolType\\": "packages/SchoolType/src",
      "Solidarity\\City\\": "packages/City/src",
      "Solidarity\\User\\": "packages/User/src",
      "Solidarity\\Delegate\\": "packages/Delegate/src"
    }
  },
  "config": {
    "preferred-install": "dist",
    "bin-dir": "vendor/bin/",
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
